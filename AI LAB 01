        "# Calculate the sum of all the values between 0-10 using while loop.\n",
        "\n",
        "sumOfNumbers = 0\n",
        "counter = 1\n",
        "while counter <= 10:\n",
        "  sumOfNumbers += counter\n",
        "  counter += 1\n",
        "print(f\"Sum of 0-10 is: {sumOfNumbers}\")"
